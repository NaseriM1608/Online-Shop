@model IEnumerable<Pooshineh.Models.Table_LoginHistory>

@{
    ViewBag.Title = "ViewLoginHistory";
}
<div class="text-center bg-primary py-5 fw-bolder rounded-1 text-white my-5"><h3>تاریخچه ورود کاربر</h3></div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LoginDate)
        </th>
       
        <th></th>
    </tr>

@foreach (var item in Model.OrderByDescending(m => m.LoginDate)) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.LoginDate)
        </td>
       
    </tr>
}

</table>
